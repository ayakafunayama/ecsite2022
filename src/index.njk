<h1>{{ title }}</h1>
<ul>
	<li><a href="./index">HOME</a></li>
	<li><a href="./photo">ABOUT</a></li>
</ul>
<img src="./images/cart_icon.png" alt="">

<!-- 各sectionをループで作成 -->
{% for topPhoto in products %}
<section class="{{ topPhoto.url }}">
	<h2>{{ topPhoto.name }}</h2>
	<div class="photo___img">
		<img src="./images/{{ topPhoto.itemphotos[0] }}" alt="{{ topPhoto.name }}" />
	</div>
</section>
{% endfor %}


